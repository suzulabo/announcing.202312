{
  "name": "announcing",
  "private": true,
  "engines": {
    "node": "=20.14.0",
    "pnpm": "=9.3.0"
  },
  "packageManager": "pnpm@9.3.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "format": "prettier --check .",
