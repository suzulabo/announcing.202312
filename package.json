    "eslint-plugin-svelte": "^2.40.0",
    "globals": "^15.6.0",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.4",
    "svelte-eslint-parser": "^0.39.2",
    "typescript": "^5.5.2",
    "typescript-eslint": "7.14.1"
  }
