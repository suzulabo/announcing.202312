{
  "name": "announcing",
  "private": true,
  "engines": {
    "node": "=22.16.0",
    "pnpm": "=10.12.1"
  },
  "packageManager": "pnpm@10.12.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint",
