{
  "name": "@announcing/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "jiti postinstall.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "generate": "drizzle-kit generate",
    "migrate:remote": "wrangler d1 migrations apply D1 -e remote --remote",
    "migrate:local": "wrangler d1 migrations apply D1 -e local --local --persist-to ../db-local",
    "genTestData": "tsx tools/genTestData.ts"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.787.0",
    "@cloudflare/workers-types": "4.20250425.0",
    "@faker-js/faker": "9.7.0",
    "@libsql/client": "0.15.4",
    "date-fns": "4.1.0",
    "dotenv": "16.5.0",
    "drizzle-kit": "0.31.0",
    "drizzle-orm": "0.42.0",
    "image-size": "2.0.2",
    "jdenticon": "3.3.0",
    "js-xxhash": "4.0.0",
    "miniflare": "4.20250424.0",
    "mockdate": "3.0.5",
    "tsx": "4.19.3",
    "vitest": "3.1.1",
    "wrangler": "4.13.2"
  }
}
