  "devDependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@faker-js/faker": "^9.6.0",
    "@libsql/client": "^0.14.0",
    "base-x": "^5.0.1",
    "buffer": "^6.0.3",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.40.0",
    "image-dimensions": "^2.3.0",
    "image-type": "^5.2.0",
    "jdenticon": "^3.3.0",
    "js-xxhash": "^4.0.0",
    "mockdate": "^3.0.5",
    "tsx": "^4.19.3",
    "vitest": "^3.0.8"
