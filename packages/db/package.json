{
  "name": "@announcing/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./constants": {
      "types": "./dist/lib/constants.d.ts",
      "import": "./dist/lib/constants.js"
    },
    "./types": {
      "types": "./dist/lib/types.d.ts",
      "import": "./dist/lib/types.js"
    },
    "./utils": {
      "types": "./dist/lib/utils.d.ts",
      "import": "./dist/lib/utils.js"
    },
    "./localDB": {
      "types": "./dist/lib/localDB.d.ts",
      "import": "./dist/lib/localDB.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "generate": "drizzle-kit generate",
    "migrateLocalDB": "mkdir -p ../db-dev && tsx tools/migrateLocalDB.ts",
    "genTestData": "tsx tools/genTestData.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@miniflare/d1": "^2.14.4",
    "@miniflare/shared": "^2.14.4",
    "@types/node": "^22.9.0",
    "base-x": "^5.0.0",
    "better-sqlite3": "^11.5.0",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.28.0",
    "drizzle-orm": "^0.36.1",
    "image-dimensions": "^2.3.0",
    "image-type": "^5.2.0",
    "mockdate": "^3.0.5",
    "tsx": "^4.19.2",
    "valibot": "1.0.0-beta.3",
    "vitest": "^2.1.4",
    "wrangler": "^3.86.0"
  }
}
