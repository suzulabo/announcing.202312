{
  "private": true,
  "name": "announcing-writer",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "wrangler:dev": "wrangler pages dev .svelte-kit/cloudflare",
    "db:push:dev": "drizzle-kit push:sqlite --config drizzie.config.dev.ts",
    "db:studio": "drizzle-kit studio --config drizzie.config.dev.ts"
  },
  "devDependencies": {
    "@announcing/components": "workspace:^",
    "@announcing/eslint-config-svelte": "workspace:^",
    "@announcing/json": "workspace:^",
    "@auth/sveltekit": "^1.0.1",
    "@libsql/client": "^0.6.0",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-cloudflare": "^4.4.0",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/psl": "^1.1.3",
    "date-fns": "^3.6.0",
    "drizzle-kit": "^0.20.17",
    "drizzle-orm": "^0.30.10",
    "linkify-html": "^4.1.3",
    "msw": "^2.2.14",
    "psl": "^1.9.0",
    "svelte": "^4.2.15",
    "svelte-check": "^3.7.1",
    "sveltekit-i18n": "^2.4.2",
    "sveltekit-superforms": "^2.13.0",
    "typescript": "^5.4.5",
    "valibot": "^0.30.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "wrangler": "^3.53.1"
  },
  "type": "module",
  "msw": {
    "workerDirectory": "static"
  }
}
